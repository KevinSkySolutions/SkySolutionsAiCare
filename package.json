{
  "name": "aicare-ashbyponds-monitoringportal",
  "version": "0.0.1",
  "description": "This is the POC for the monitoring portal for AiCare.",
  "main": "src/index.js",
  "scripts": {
    "lintJS": "eslint --fix ./src",
    "lintCSS": "stylelint src/**/*.css",
    "precommit": "npm run lintJS --loglevel=silent && npm run lintCSS --loglevel=silent",
    "browser": "cross-env NODE_ENV=development babel-node ./server.js",
    "build": "webpack --config ./webpack.config.prod.js",
    "server": "npm run build && cross-env NODE_ENV=production babel-node ./server.js",
    "transpile-server": "rimraf dist && cpy ./server/views/*.* ./dist/views && cpy ./server/views/**/*.* ./dist/views/layouts && cpy ./server/mock-data/*.* ./dist/mock-data && tsc -p \"./tsconfig.json\"",
    "devBuild": "webpack --config ./webpack.config.dev.js",
    "start": "npm run devBuild && npm run transpile-server && node ./dist/spawn-server/server.js"
  },
  "author": "Rohit",
  "dependencies": {
    "axios": "0.15.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "image-webpack-loader": "^4.2.0",
    "lodash": "4.16.4",
    "npm": "^5.8.0",
    "prop-types": "^15.6.1",
    "rc-dialog": "^7.1.4",
    "react": "^16.0.0",
    "react-dom": "^16.3.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.1",
    "react-router-redux": "4.0.6",
    "redux": "3.6.0",
    "redux-logger": "2.7.0",
    "redux-observable": "^0.18.0",
    "request": "^2.79.0",
    "rxjs": "^5.5.8",
    "app-root-path": "^2.0.1",
    "body-parser": "^1.18.2",
    "cluster": "^0.7.7",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "http": "0.0.0",
    "log4js": "^2.5.3",
    "method-override": "^2.3.10",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/node": "^10.0.4",
    "autoprefixer": "6.3.7",
    "babel-cli": "6.10.1",
    "babel-core": "6.10.4",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-lodash": "3.2.9",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.5.0",
    "clean-webpack-plugin": "0.1.13",
    "copy-webpack-plugin": "3.0.1",
    "cpy-cli": "^1.0.1",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "css-mqpacker": "5.0.1",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "1.16.0",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "forever": "^0.15.3",
    "jsdom": "9.8.0",
    "node-sass": "^4.8.3",
    "react-addons-test-utils": "15.3.2",
    "react-test-renderer": "15.3.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "style-loader": "0.13.1",
    "stylelint": "7.5.0",
    "ts-node": "^6.0.3",
    "tsconfig-paths": "^3.3.1",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  }
}
